# Database Configuration
DB_HOST=localhost
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=agent_swarm_db
DB_PORT=5432

# API Configuration
PORT=3000
NODE_ENV=development

# Language Model Configuration
OPENAI_API_KEY=your-api-key-here
CLAUDE_API_KEY=your-api-key-here
COHERE_API_KEY=your-api-key-here

# Memory Configuration
MEMORY_PROVIDER=redis  # or postgres, chroma, pinecone
REDIS_URL=redis://localhost:6379
POSTGRES_URL=postgresql://user:password@localhost:5432/memory_db

# Tool Configuration
TOOL_EXECUTION_TIMEOUT=30000  # milliseconds
MAX_CONCURRENT_TOOLS=5

# Swarm Configuration
DEFAULT_SWARM_TYPE=SequentialWorkflow
MAX_SWARM_LOOPS=5
COLLABORATION_THRESHOLD=0.7

# Security
JWT_SECRET=your-jwt-secret-here
API_KEY=your-api-key-here

# Logging
LOG_LEVEL=debug  # debug, info, warn, error
LOG_FORMAT=json  # json, text

# Monitoring
ENABLE_METRICS=true
PROMETHEUS_PORT=9090

# Rate Limiting
RATE_LIMIT_WINDOW=15m
RATE_LIMIT_MAX_REQUESTS=100

# API Keys
BIRDEYE_API_KEY=
COVEQUANT_API_KEY=